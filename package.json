{
  "name": "jwerly-api",
  "packageManager": "yarn@3.0.2",
  "scripts": {
    "lint": "prettier -c \"./**/*.ts\";eslint **/*.ts",
    "format": "prettier --write \"./**/*.ts\" --ignore-path \"./node_modules/**/*.ts\";eslint --fix",
    "watch": "nodemon -e ts -w . -x node dist/index.js",
    "build": "tsc",
    "dev": "ts-node index.ts",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "devDependencies": {
    "@types/aws-serverless-express": "^3",
    "@types/body-parser": "^1",
    "@types/compression": "^1",
    "@types/cors": "^2",
    "@types/eslint": "^7",
    "@types/express": "^4",
    "@types/express-oauth-server": "^2",
    "@types/jsonwebtoken": "^8",
    "@types/oauth2-server": "^3",
    "@types/passport": "^0",
    "@types/passport-http-bearer": "^1",
    "@types/pg": "^8",
    "@types/prettier": "^2",
    "@types/swagger-jsdoc": "^6",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "swagger-jsdoc": "^6.1.0",
    "typescript": "^4.4.3"
