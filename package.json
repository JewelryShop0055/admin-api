    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-oauth-server": "^2.0.0",
    "helmet": "^4.6.0",
    "json-ignore": "^0.4.0",
    "jsonwebtoken": "^8.5.1",
    "oauth2-server": "^3.1.1",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
